<?xml version="1.0"?>
<xtpl:template xmlns:xtpl="http://rubaxa.org/">
    <xtpl:include src="portal-headline.xml"/>
    <xtpl:include src="portal-menu.xml"/>
    <xtpl:include src="left-col.xml"/>
    <xtpl:include src="list.xml"/>
    <xtpl:set name="content">
        <xtpl:get name="headline"/>
        <xtpl:get name="headline-menu"/>
        <div class="page minwidth">
            <xtpl:get name="left-col"/>
            <div class="page__content">
                <div class="page__content__wrapper">
                    <xtpl:get name="list"/>
                </div>
            </div>
        </div>
    </xtpl:set>
</xtpl:template>

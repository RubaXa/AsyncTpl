            .portal-headline__project__link_icon:focus {
              padding-left: 30px; }

            .portal-headline__project__link_icon__pic {
              position: absolute;
              width: 18px;
              height: 18px;
              margin-top: -2px;
              margin-left: -22px;
              background-image: url(http://img.imgsmail.ru/r/portal/portal-headline/portal-headline__project__link_icon.png);
              background-repeat: no-repeat; }

            .portal-headline__project__link_icon__pic_agent {
              background-position: 0 2px; }

            .portal-headline__project__link_icon__pic_sputnik {
              background-position: 0 -18px; }

            .portal-headline__project__link_icon__pic_icq {
              background-position: 0 -39px; }

            .portal-headline__login {
              position: absolute;
              top: 7px;
              *top: 8px;
              right: 18px;
              font-size: 12px;
              line-height: 14px; }

            .portal-headline__login__link,
            .portal-headline__login__link:link,
            .portal-headline__login__link:visited,
            .portal-headline__login__link:hover,
            .portal-headline__login__link:active,
            .portal-headline__login__link:focus {
              margin-left: 10px;
              color: #0857a6;
              text-decoration: none; }

            .portal-headline__login__link:hover,
            .portal-headline__login__link:active {
              color: #f26d00;
              text-decoration: none; }
        </style>

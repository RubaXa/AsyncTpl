        <style type="text/css">
            .portal-headline {
              position: relative;
              z-index: 1003;
              padding-top: 7px;
              padding-bottom: 7px;
              font-family: Arial, sans-serif;
              font-size: 12px;
              text-align: left;
              background: #fff;
              *zoom: 1; }

            .portal-headline__link,
            .portal-headline__link:link,
            .portal-headline__link:visited,
            .portal-headline__link:hover,
            .portal-headline__link:active,
            .portal-headline__link:focus {
              display: inline;
              margin-right: 18px;
              color: #0857a6;
              font-size: 12px;
              line-height: 14px;
              text-decoration: none;
              white-space: nowrap;
              *zoom: 1; }

            .portal-headline__link:hover,
            .portal-headline__link:active {
              color: #f26d00; }

            .portal-headline__link_first,
            .portal-headline__link_first:link,
            .portal-headline__link_first:visited,
            .portal-headline__link_first:hover,
            .portal-headline__link_first:active,
            .portal-headline__link_first:focus {
              margin-left: 18px; }

            .portal-headline__link_ext,
            .portal-headline__link_ext:link,
            .portal-headline__link_ext:visited,
            .portal-headline__link_ext:hover,
            .portal-headline__link_ext:active,
            .portal-headline__link_ext:focus {
              margin-right: 5px;
              text-decoration: none; }

            .portal-headline__link_selected {
              font-weight: bold; }

            .portal-headline__link__text {
              font-style: normal;
              text-decoration: none;
              cursor: pointer; }

            .portal-headline__link:hover .portal-headline__link__text {
              color: #f26d00;
              text-decoration: none; }

            .portal-headline__link__balloon {
              display: none;
              min-width: 9px;
              margin-right: 5px;
              margin-left: 3px;
              padding-right: 4px;
              padding-left: 4px;
              color: #fff;
              font-weight: bold;
              font-style: normal;
              font-size: 11px;
              line-height: 12px;
              text-align: center;
              background-color: #fa0;
              -webkit-border-radius: 2px;
              -moz-border-radius: 2px;
              border-radius: 2px;
              cursor: pointer; }

            .portal-headline__link_ext .portal-headline__link__balloon {
              display: inline-block;
              *display: inline;
              *zoom: 1; }

            .portal-headline__projects {
              position: relative;
              display: inline-block;
              *display: inline;
              *zoom: 1;
              font-size: 12px;
              line-height: 1; }

            .portal-headline__projects__link,
            .portal-headline__projects__link:link,
            .portal-headline__projects__link:visited,
            .portal-headline__projects__link:hover,
            .portal-headline__projects__link:active,
            .portal-headline__projects__link:focus {
              text-decoration: none;
              outline: 0;
              cursor: pointer;
              *zoom: 1; }

            .portal-headline__projects__link__text,
            .portal-headline__projects_open .portal-headline__projects__link:hover .portal-headline__projects__link__text {
              display: inline-block;
              *display: inline;
              *zoom: 1;
              border-bottom: 1px dotted #fff;
              color: #0857a6;
              font-style: normal;
              text-decoration: none;
              *margin-bottom: 1px;
              *padding-bottom: 1px;
              *vertical-align: bottom; }

            .portal-headline__projects__link:hover .portal-headline__projects__link__text {
              border-bottom: 1px dotted #fff;
              color: #f26d00; }

            .portal-headline__projects__link__icon,
            .portal-headline__projects_open .portal-headline__projects__link:hover .portal-headline__projects__link__icon {
              position: relative;
              top: 3px;
              display: inline-block;
              *display: inline;
              *zoom: 1;
              width: 0;
              height: 0;
              font: 0/0 a;
              border-width: 4px;
              border-style: solid;
              border-color: #0857a6 #fff #fff; }

            .portal-headline__projects__link:hover .portal-headline__projects__link__icon {
              border-color: #f26d00 #fff #fff; }

            .portal-headline__projects_open {
              margin-top: -2px;
              margin-right: -6px;
              margin-bottom: -6px;
              margin-left: -6px;
              *margin-bottom: -7px; }

            .portal-headline__projects_open .portal-headline__projects__link {
              position: relative;
              z-index: 1005;
              display: inline-block;
              padding-top: 1px;
              padding-right: 5px;
              padding-bottom: 6px;
              padding-left: 5px;
              *padding-bottom: 4px;
              border: 1px solid #9fc1eb;
              border-bottom: none;
              -webkit-border-radius: 2px 2px 0 0;
              -moz-border-radius: 2px 2px 0 0;
              border-radius: 2px 2px 0 0;
              background: #fff; }

            .portal-headline__projects__wrap {
              position: absolute;
              z-index: 1004;
              top: 20px;
              left: 0;
              display: none;
              width: 380px;
              border: 1px solid #9fc1eb;
              -webkit-border-radius: 2px;
              -moz-border-radius: 2px;
              border-radius: 2px;
              box-shadow: 0 4px 4px #656972;
              background: #fff; }

            .portal-headline__projects_open .portal-headline__projects__wrap {
              display: block;
              -webkit-animation-duration: 0.5s;
              -moz-animation-duration: 0.5s;
              animation-duration: 0.5s;
              -webkit-animation-name: slidein;
              -moz-animation-name: slidein;
              animation-name: slidein; }

            @-webkit-keyframes slidein {
              0% {
                opacity: 0;
                box-shadow: none; }

              100% {
                opacity: 1;
                box-shadow: 0 4px 4px #656972; } }

            @-moz-keyframes slidein {
              0% {
                opacity: 0;
                box-shadow: none; }

              100% {
                opacity: 1;
                box-shadow: 0 4px 4px #656972; } }

            @keyframes slidein {
              0% {
                opacity: 0;
                box-shadow: none; }

              100% {
                opacity: 1;
                box-shadow: 0 4px 4px #656972; } }

            .portal-headline__projects__column {

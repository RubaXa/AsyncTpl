<?xml version="1.0"?>
<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <fest:doctype/>
    <html>
        <head>
            <title>Fest bench</title>
            <meta http-equiv="content-type" content="text/html; charset=utf-8" />
        </head>
        <body>
            <fest:script><![CDATA[
                ctx.list = (function(){var array=[]; for(var i=0;i<100;i++){array.push(i)}return array})();
            ]]></fest:script>
            <fest:foreach iterate="ctx.list">
                <fest:foreach iterate="ctx.checknew.messages" as="message">
                    <a href="fest:message.Id" style="display:block"><fest:value>message.Subject</fest:value></a>
                </fest:foreach>
            </fest:foreach>
        </body>
    </html>
</fest:template>
